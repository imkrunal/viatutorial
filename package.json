{
  "name": "viatutorial",
  "private": true,
  "description": "Tutorials Website",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "format": "prettier \"**/*.{md,mdx,yml} \" --write",
    "cy:open": "cypress open",
    "cy:run": "CYPRESS_baseUrl=http://localhost:9000 cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --fix --ignore-path .gitignore",
    "lint:ci": "yarn lint --format junit -o results/eslint/result.xml",
    "test": "jest",
    "serve-and-run": "start-server-and-test serve http://localhost:9000 cy:run",
    "test:e2e": "cross-env CYPRESS_SUPPORT=y yarn build && yarn serve-and-run",
    "test:lighthouse": "start-server-and-test serve http://localhost:9000 test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.14",
    "@mdx-js/mdx": "1.0.21",
    "@mdx-js/react": "1.0.21",
    "@mdx-js/tag": "0.20.3",
    "emotion-theming": "10.0.14",
    "gatsby": "2.13.27",
    "gatsby-image": "2.2.6",
    "gatsby-mdx": "0.6.3",
    "gatsby-plugin-catch-links": "2.1.2",
    "gatsby-plugin-emotion": "4.1.2",
    "gatsby-plugin-google-analytics": "2.1.4",
    "gatsby-plugin-manifest": "2.2.3",
    "gatsby-plugin-netlify": "2.1.3",
    "gatsby-plugin-netlify-cache": "1.2.0",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.2.8",
    "gatsby-plugin-sitemap": "2.2.3",
    "gatsby-plugin-typography": "2.3.2",
    "gatsby-remark-code-titles": "1.1.0",
    "gatsby-remark-copy-linked-files": "2.1.3",
    "gatsby-remark-images": "3.1.6",
    "gatsby-source-filesystem": "2.1.5",
    "gatsby-transformer-sharp": "2.2.4",
    "gatsby-transformer-yaml": "2.2.3",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "prism-react-renderer": "0.1.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-typography": "0.16.19",
    "slugify": "1.3.4",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@testing-library/cypress": "4.0.4",
    "babel-eslint": "10.0.2",
    "chrome-launcher": "0.11.1",
    "cli-table": "0.3.1",
    "cross-env": "5.2.0",
    "cypress": "3.4.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "gatsby-cypress": "0.2.3",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "lighthouse": "5.1.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "start-server-and-test": "1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imkrunal/viatutorial"
  },
  "bugs": {
    "url": "https://github.com/imkrunal/viatutorial/issues"
  }
}
